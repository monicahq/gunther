{
    "name": "monicahq/gunther",
    "description": "Update Laravel translations on Crowdin",
    "keywords": ["laravel", "php", "crowdin", "language", "i18n"],
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "support": {
        "issues": "https://github.com/monicahq/gunther/issues",
        "source": "https://github.com/monicahq/gunther"
    },
    "authors": [
        {
            "name": "Alexis Saettler",
            "email": "alexis@saettler.org"
        }
    ],
    "require": {
        "caouecs/laravel-lang": "^6.0 || ^7.0",
        "elkuku/crowdin-api": "^2.0",
        "illuminate/console": "^5.6 || ^6.0 || ^7.0",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0",
        "umpirsky/locale-list": "^1.0"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.2.0",
        "laravel/framework": "^5.6 || ^6.0 || ^7.0",
        "nunomaduro/larastan": "^0.5",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Gunther\\": "src",
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gunther\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
